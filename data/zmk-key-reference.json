{
  "behaviors": [
    {
      "name": "&kp",
      "description": "Send a keycode when pressed.",
      "params": ["keycode"]
    },
    {
      "name": "&mt",
      "description": "Sends a different key press depending on whether a key is held or tapped. (Mod Tap)",
      "params": ["modifier", "keycode"]
    },
    {
      "name": "&lt",
      "description": "Enables a layer when a key is held, and outputs a key press when the key is only tapped for a short time. (Layer Tap)",
      "params": ["layer", "keycode"]
    },
    {
      "name": "&mo",
      "description": "Enables a layer while a key is pressed. (Momentary Layer)",
      "params": ["layer"]
    },
    {
      "name": "&hml",
      "description": "Enables a layer when a key is held, and outputs a key press when the key is only tapped for a short time. (Layer Tap)",
      "params": ["layer", "keycode"]
    },
    {
      "name": "&hmr",
      "description": "Enables a layer when a key is held, and outputs a key press when the key is only tapped for a short time. (Layer Tap)",
      "params": ["layer", "keycode"]
    },
    {
      "name": "&to",
      "description": "Enables a layer and disables all other layers except the default layer.",
      "params": ["layer"]
    },
    {
      "name": "&tog",
      "description": "Enables a layer until the layer is manually disabled. (Toggle Layer)",
      "params": ["layer"]
    },
    {
      "name": "&sl",
      "description": "Activates a layer until another key is pressed, then deactivates it. (Sticky Layer)",
      "params": ["layer"]
    },
    {
      "name": "&sk",
      "description": "Stays pressed until another key is pressed, then is released. (Sticky Key)",
      "params": ["modifier"]
    },
    {
      "name": "&bt",
      "description": "Bluetooth control.",
      "params": ["bt_command"]
    },
    {
      "name": "&rgb_ug",
      "description": "RGB Underglow control.",
      "params": ["rgb_command"]
    },
    {
      "name": "&out",
      "description": "Output selection (USB/BLE).",
      "params": ["output_command"]
    },
    {
        "name": "&trans",
        "description": "Transparent. Passes the key press down to the next active layer.",
        "params": []
    },
    {
        "name": "&none",
        "description": "Disables a key. No keycode will be sent.",
        "params": []
    }
  ],
